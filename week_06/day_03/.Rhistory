infer_resample <- houses %>%
specify(response = pre_1920, success = "yes") %>%
generate(reps = analysis$samples[isamp], type = "bootstrap") %>%
calculate(stat = "prop")
# calc ci 95
infer_ci_95 <- infer_resample %>%
get_confidence_interval(level = 0.95, type = "percentile")
analysis$results_ci_upper[isamp] <- infer_ci_95$upper_bound %>%
pull()
analysis$results_ci_lower[isamp] <- infer_ci_95$lower_bound %>%
pull()
#visualise
infer_resample %>%
visualise(bins=30) +
shade_confidence_interval(endpoints = infer_ci_95)
